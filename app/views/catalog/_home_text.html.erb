<div id="getting-started">
  <h2 class='page-heading'><i class="fa fa-search fa-10x"></i></h2>
  <div class="divider"></div>
  <h3 class='section-heading'>Try a new search</h3>
  <div class="divider"></div>
  <p class="description">Some Examples to get started:</p>
  <p><tt>Apple AND Computers</tt></p>
  <p><tt>Wolverine NOT Football</tt></p>
  <p><tt>DIAG + Squirrels</tt></p>
  
</div>
<script>
  $(function() {
    $('#about a').on('click', function(e) {
      e.preventDefault();
      if ($('#about-content').html() == "") {
        $('#about-content').load($(this).attr("href"), function() {
          $('#about-content').show();
        });
      } else {
        $('#about-content').toggle();
      }
    });
  });
</script>
