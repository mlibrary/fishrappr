<h3>
    <%= issue_title(document, true) %>
    - 
     Image <%= current_image_sequence(document) %>
</h3>
<dl class="row">
    <dt class="col-3">Resource type:</dt>
    <dd class="col-9">Text</dd>
    <dt class="col-3">Publication:</dt>
    <dd class="col-9"><%= document.fetch('publication_label') %></dd>
    <% if document.fetch('page_no_t', []).first %>
    <dt class="col-3">Page Number:</dt>
    <dd class="col-9"><%= document.fetch('page_no_t').first %></dd>
    <% end %>
    <!-- <dt>Division:</dt>
    <dd>Bentley Historical Library</dd> -->
    <% if document.fetch('page_label', nil) == 'tabloid' %>
    <dt class="col-3">Note:</dt>
    <dd class="col-9"><span class="label label-note">This is a <a href="<%= fishrappr_path('using_page_viewer') %>#tabloid-pages" target="_blank">tabloid page</a></dd></dd>
    <% end %>
</dl>
<hr />